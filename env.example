# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./logs/app.log
TRUST_PROXY_HEADERS=0

# YouTube extraction overrides
YT_COOKIES_FILE=/path/to/cookies.txt
YT_COOKIES_FROM_BROWSER=chrome
YT_USER_AGENT=Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36
YT_PROXY=http://user:pass@proxy.example.com:8080

# Integration testing
RUN_YT_INTEGRATION=0
YOUTUBE_TEST_URL=https://www.youtube.com/watch?v=WRvWLWfv4Ts
